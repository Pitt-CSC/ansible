{
  "checks": {
    {% for check in sensu_checks %}
      "{{ check.name }}": {
        "handlers": {{ check.handlers|to_json }}
        "command": {{ check.command|to_json }},
        "interval": {{ check.interval|to_json }},
        "subscribers": {{ check.subscribers|to_json }}
      }{% if not loop.last %},{% endif %}
    {% endfor %}
  }
}

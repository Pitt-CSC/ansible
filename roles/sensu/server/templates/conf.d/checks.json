{
  "checks": {
    {% for check in sensu_checks %}
      "{{ check.name }}": {
        "handlers": {{ check.handlers|tojson }}
        "command": {{ check.command|tojson }},
        "interval": {{ check.interval|tojson }},
        "subscribers": {{ check.subscribers|tojson }}
      }{% if not loop.last %},{% endif %}
    {% endfor %}
  }
}
